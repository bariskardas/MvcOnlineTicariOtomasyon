@using MvcTicariOtomasyon.Models.Siniflar
@model List<Personel>
@{
    ViewBag.Title = "DepartmanDetay";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>Departman Detay Sayfası</h2>
<br />
<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active"> @ViewBag.u Departmanı</li>
</ol>

<table class="table table-hover">
    <tr>
        <th>Personel Adı</th>
        <th>Satışlar</th>
    </tr>
    @foreach (var x in Model)
    {
     <tr>
         <td>@x.PersonelAd @x.PersonelSoyad</td>
          <td><a href="/Departman/DepartmanPersonelSatis/@x.Personelid" class="btn btn-primary">Satışlar</a></td>
     </tr>
    }
</table>

